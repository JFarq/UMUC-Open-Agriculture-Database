<div class="row">
  <div class="col-md-8">
    <div>
      <%= render 'shared/user_info' %>
    </div>
    <div class="well">
      <h3>Find Friends</h3>
      <hr>
      <div id="users">
        <%= render(@users) || render('shared/no_resource', resources: 'Users') %>
      </div>
      <%= render 'shared/paginate', resources: @users %>
    </div>
  </div>
  <div class="col-md-4">
    <div>
      <%= render 'shared/links' %>
    </div>
  </div>
</div>
